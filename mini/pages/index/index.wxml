<!--logs.wxml-->
<view class="home">
  <view class="page-section page-section-spacing swiper">
    <swiper 
      class="swiper" 
      indicator-dots="true" 
      autoplay="true" 
      circular="true"
      interval="2000" 
      duration="500"
      >
      <block wx:for="{{swiperImg}}" wx:key="*this">
        <swiper-item>
          <image src="{{item}}" mode="scaleToFill"></image>
        </swiper-item>
      </block>
    </swiper>
  </view>
  <view class="book-items-container">
    <view class="book-item" wx:for="{{books}}">
      <!-- <bookItem bookid = "{{item.id}}" imgUrl = "{{item.imgUrl}}"/> -->
    </view>
  </view>
</view>
